  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write .",
      "eslint --fix --color"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "lint": "eslint src --config eslint.config.mjs",
    "lint:fix": "eslint src --config eslint.config.mjs --fix --color",
    "format": "prettier --write .",
    "start:prod": "npm run format && npm run lint:fix && pm2-runtime start ecosystem.config.js --watch",
    "prepare": "husky && husky install",
    "ts-node": "ts-node --compiler-options '{\"module\": \"CommonJS\"}'",
    "db-clear": "ts-node prisma/clear.ts",
    "migrate:down": "npx prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > prisma/DownMigrations/down.sql",
    "start:dev": "npm run format && npm run lint:fix && nodemon src/server.ts",
    "migrate:create-only": "npx prisma migrate dev --create-only",
    "test:e2e": "npx cypress run",
    "test:unit": "mocha"
  }
}
